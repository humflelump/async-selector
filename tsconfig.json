{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2017", "es7", "es6", "dom"],
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react",
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "exclude": ["node_modules", "dist"]
}
